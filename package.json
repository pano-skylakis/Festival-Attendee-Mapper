{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "starter for webpack React",
  "main": "server/index.js",
  "author": "Pano",
  "license": "ISC",
  "scripts": {
    "client": "webpack-dev-server --hot --inline",
    "dev": "npx run-p run-client run-server",
    "run-client": "webpack --watch",
    "run-server": "nodemon server",
    "server": "node server",
    "start": "webpack && node server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "scss": "node-sass --watch client/sass -o public/css",
    "postinstall": "webpack && knex migrate:latest"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "aos": "^2.3.4",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "heroku": "^7.26.2",
    "jsdom": "^15.1.1",
    "knex": "^0.19.5",
    "leaflet": "^1.5.1",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "pg": "^7.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-google-charts": "^3.0.14",
    "react-leaflet": "^2.4.0",
    "react-leaflet-heatmap-layer": "^2.0.0",
    "react-loader-spinner": "^3.1.2",
    "react-promise-tracker": "^2.0.4",
    "react-scroll": "^1.7.12",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.22.1",
    "sass-loader": "^7.1.0",
    "sqlite3": "^4.0.9",
    "superagent": "^5.1.0",
    "uuid": "^3.3.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2"
  },
  "devDependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pano-skylakis/boilerplate-react-webpack.git"
  },
  "bugs": {
    "url": "https://github.com/pano-skylakis/boilerplate-react-webpack/issues"
  },
  "homepage": "https://github.com/pano-skylakis/boilerplate-react-webpack#readme"
}
